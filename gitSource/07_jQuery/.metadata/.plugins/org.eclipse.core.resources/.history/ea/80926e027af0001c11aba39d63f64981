	$(document).read(function(){//window.onload랑 비슷한 맥락
	 	//function 안에다가 j쿼리 문법들이 쭉 들어가면 됨 	
		//$('#p1') = document.getElement 한거랑 동일 
	 	$('#p1').click(function(){});
	});  
	
	window.onload = function(){
		// 자바스크립트 소스 
	};
	
</script>
