//				jta.setText("회원번호\t전화번호\t이름\t포인트\n");
				jta.setText(dtos.toString());
			}
			
			
			
		}else if(e.getSource()==btnOutput) {
			
		}else if(e.getSource()==btnExit) {
			setVisible(false);
			dispose();
			System.exit(0);
		}
		
		
		
		
		

	}
